import java.util.Scanner;
public class Tabuada {
	public static void main(String[] arges) {
		Scanner in = new Scanner (System.in);
		int i=1;
		double n,t;
		System.out.println("Digite um n√∫mero");
		n = in.nextDouble();
		
		while (i<=10) {
			t = n*i;
			System.out.println(n+"x"+i+"="+t);
			i=i+1;
		}


		in.close();
}
}
