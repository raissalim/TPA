import java.util.;
public class Ativ1 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Random ler = new Random();
		int A[] [] =new int [4] [4];
		int j,i;
		A [0] [0] = 2;
		
	ia:	for ( i = 0; i < A.length; i++) {
			for ( j = 0; j < A.length; j++) {
				A[i][j] = A[j][i]*2;
				System.out.println(A[i] [j]);
				A [i][j]=ler.nextInt();
				continue ia;
			}
		}
		
	}
	
}
