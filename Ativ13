import java.util.Scanner;
public class Ativ13 {
	public static void main(String[] args) {
		Scanner ler = new Scanner(System.in);
			final int TAM = 10;
			int a[],b[];
			a = new int [TAM];
			b = new int [TAM];
			
			for (int i=0; i<TAM; i++) {
				a[i] = ler.nextInt();
			}
			
			for (int i =0; i<TAM; i++) {
				if(a[i]%2==0) {
					b[i]=1;
				}else {
					b[i] = 0;
				}
			}
			for (int i =0; i<TAM; i++) {
				System.out.println(b[i]);
			}
			ler.close();
}
}
