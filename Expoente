                                   
import java.util.Scanner;
public class Base {
	public static void main(String[] arges) {
		Scanner in = new Scanner(System.in);
		int N,Ex, Potencia,Contador;
		
		System.out.println("Digite um numero:");
		N = in.nextInt();
		
		System.out.println("Digite o expoente");
		Contador= in.nextInt();
		Ex=N;
		
		Contador=Contador-1;
		while (Contador>=1) {
			Potencia=N*Ex;
			System.out.println(N+"x"+Ex+"="+Potencia);
			N=Potencia;
			Contador=Contador-1;
		}

		
		in.close();
}
}
